<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.bintang.bangkitcapstoneproject.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <activity
        android:id="@+id/navigation_food_detector"
        android:name="com.bintang.bangkitcapstoneproject.ui.food_detector.FoodDetectorCamera"
        android:label="activity_food_detector_camera"
        tools:layout="@layout/activity_food_detector_camera" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.bintang.bangkitcapstoneproject.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

</navigation>